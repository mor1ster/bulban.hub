        Duration = 5 -- Set to nil to make the notification not disappear  
    })  

    Tabs.Main:AddParagraph({  
        Title = "Paragraph",  
        Content = "This is a paragraph.\nSecond line!"  
    })  
end -- Закрывающая скобка для блока do

