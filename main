    Tabs.Main:AddButton({
        Title = "Sell all",
        Description = "Selling all fish in your inventory",
        Callback = function()
            Window:Dialog({
                Title = "are you sure?",
                Content = "Are you sure you want to sell all the fish in your inventory",
                Buttons = {
                    {
                        Title = "Confirm",
                        Callback = function()
                            local ohString1 = "SellEverything"

                            game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.processGameItemSold:InvokeServer(ohString1)
                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function()
                            print("You don't want to sell your fish")
                        end
                    }
                }
            })
        end
    })

    local SellEverythingToggle = Tabs.Main:AddToggle("SellEverythingLoop", {  
    Title = "Sell Everything Loop",  
    Default = false  
})

local ohString1 = "SellEverything"  

-- Functionality for auto selling items  
SellEverythingToggle.Callback = function(value)  
    if value then  
        -- Start the loop when the toggle is enabled  
        spawn(function()  
            while SellEverythingToggle do  
                -- Invoke server function to sell everything  
                game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.processGameItemSold:InvokeServer(ohString1)  
                
                -- Wait before the next invocation  
                wait(1)  -- Adjust the waiting time as necessary  
            end  
        end)  
    end  
end

local FishingToggle = Tabs.Main:AddToggle("FishingToggle", {  
    Title = "auto catching",  
    Default = false  
})  

local fishingLoop -- Переменная для хранения задачи  

FishingToggle:OnChanged(function(value)  
    if value then  
        print("Fishing активирован")  

        -- Запускаем цикл для проверки видимости элемента  
        fishingLoop = task.spawn(function()  
            while true do -- Необходим бесконечный цикл  
                if not FishingToggle then -- Проверяем состояние тумблера  
                    print("Fishing деактивирован")  
                    break -- Выход из цикла, если тумблер выключен  
                end  

                local player = game:GetService("Players").LocalPlayer  
                if player and player.PlayerGui and player.PlayerGui.Interface and player.PlayerGui.Interface.Fishing and player.PlayerGui.Interface.Fishing.FishingMeter then  
                    local fishingMeterVisible = player.PlayerGui.Interface.Fishing.FishingMeter.Visible == true  
                    
                    if fishingMeterVisible then  
                        game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.FishCaught:FireServer()  
                        wait(1) -- Ждем 1 секунду перед следующим вызовом  
                    else  
                        print("FishingMeter невиден, вызов функции FishCaught пропущен")  
                        wait(1) -- Ждем 1 секунду, если FishingMeter невиден  
                    end  
                else  
                    print("FishingMeter не найден")  
                end  

                wait(3) -- Ждем 1 секунду перед следующей проверкой видимости  
            end  
        end)  
    else  
        print("Fishing деактивирован")  
        if fishingLoop then  
            task.cancel(fishingLoop) -- Останавливаем цикл  
            fishingLoop = nil -- Сброс переменной  
        end  
    end  
end)

local fishbitingToggle = Tabs.Main:AddToggle("fishbitingToggle", {  
    Title = "auto fishbiting",  
    Default = false  
})  

local fishbitingLoop -- Переменная для хранения задачи  

fishbitingToggle:OnChanged(function(value)  
    if value then  
        print("Fishing активирован")  

        -- Запускаем цикл для проверки видимости элемента  
        fishbitingLoop = task.spawn(function()  
            while true do -- Необходим бесконечный цикл  
                if not fishbitingToggle then -- Проверяем состояние тумблера  
                    print("Fishing деактивирован")  
                    break -- Выход из цикла, если тумблер выключен  
                end  

                local player = game:GetService("Players").LocalPlayer  
                if player and player.PlayerGui and player.PlayerGui.Interface and player.PlayerGui.Interface.Fishing and player.PlayerGui.Interface.Fishing.FishingMeter then  
                    local fishbitinginvisible = player.PlayerGui.Interface.Fishing.FishingMeter.Visible == false
                    
                    if fishbitinginvisible then  
                        game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.FishBiting:InvokeServer() 
                    else  
                        print("FishingMeter невиден, вызов функции FishCaught пропущен")  
                    end  
                else  
                    print("FishingMeter не найден")  
                end  
                
                wait(0.25) -- Ждем 1 секунду перед следующей проверкой  
            end  
        end)  
    else  
        print("Fishing деактивирован")  
        if fishbitingLoop then  
            task.cancel(fishbitingLoop) -- Останавливаем цикл  
            fishbitingLoop = nil -- Сброс переменной  
        end  
    end  
end)


